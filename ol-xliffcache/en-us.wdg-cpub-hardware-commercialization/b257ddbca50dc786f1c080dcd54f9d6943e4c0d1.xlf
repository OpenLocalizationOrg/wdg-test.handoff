      <unit id="104">
        <segment state="initial">
          <source>In Windows Performance Analyzer (WPA), you can provide feedback by sending a smile or frown.</source>
          <target>In Windows Performance Analyzer (WPA), you can provide feedback by sending a smile or frown.</target>
        </segment>
      </unit>
      <unit id="105">
        <segment state="initial">
          <source>To provide feedback</source>
          <target>To provide feedback</target>
        </segment>
      </unit>
      <unit id="106">
        <segment state="initial">
          <source>In the upper right corner of Windows Performance Analyzer, click the smiley face.</source>
          <target>In the upper right corner of Windows Performance Analyzer, click the smiley face.</target>
        </segment>
      </unit>
      <unit id="107">
        <segment state="initial">
          <source>A menu opens with two options.</source>
          <target>A menu opens with two options.</target>
        </segment>
      </unit>
      <unit id="108">
        <originalData>
          <data id="id1">**</data>
          <data id="id2">**</data>
          <data id="id3">**</data>
          <data id="id4">**</data>
        </originalData>
        <segment state="initial">
          <source>Select <pc dataRefEnd="id2" dataRefStart="id1" id="p1">Send a Smile...</pc> or <pc dataRefEnd="id4" dataRefStart="id3" id="p2">Send a Frown...</pc>.</source>
          <target>Select <pc dataRefEnd="id2" dataRefStart="id1" id="p1">Send a Smile...</pc> or <pc dataRefEnd="id4" dataRefStart="id3" id="p2">Send a Frown...</pc>.</target>
        </segment>
      </unit>
      <unit id="109">
        <segment state="initial">
          <source>The Microsoft Windows Performance Analyzer Feedback dialog opens; the text in the dialog corresponds to the option you selected (smile or frown).</source>
          <target>The Microsoft Windows Performance Analyzer Feedback dialog opens; the text in the dialog corresponds to the option you selected (smile or frown).</target>
        </segment>
      </unit>
      <unit id="110">
        <segment state="initial">
          <source>Note</source>
          <target>Note</target>
        </segment>
      </unit>
      <unit id="111">
        <segment state="initial">
          <source>Steps 3 through 5 are optional; however, any feedback provided is helpful.</source>
          <target>Steps 3 through 5 are optional; however, any feedback provided is helpful.</target>
        </segment>
      </unit>
      <unit id="112">
        <segment state="initial">
          <source>Provide feedback in the message area.</source>
          <target>Provide feedback in the message area.</target>
        </segment>
      </unit>
      <unit id="113">
        <originalData>
          <data id="id1">**</data>
          <data id="id2">**</data>
        </originalData>
        <segment state="initial">
          <source>Check <pc dataRefEnd="id2" dataRefStart="id1" id="p1">Include screenshot</pc> to include the accompanying screenshot of WPA, which WPA automatically creates when you opt to provide feedback.</source>
          <target>Check <pc dataRefEnd="id2" dataRefStart="id1" id="p1">Include screenshot</pc> to include the accompanying screenshot of WPA, which WPA automatically creates when you opt to provide feedback.</target>
        </segment>
