          <data id="id2">**</data>
        </originalData>
        <segment state="initial">
          <source>
            <pc dataRefEnd="id2" dataRefStart="id1" id="p1">On/Off - Standby/Resume</pc>: Records performance when the computer is placed on standby, and then resumed.</source>
          <target>
            <pc dataRefEnd="id2" dataRefStart="id1" id="p1">On/Off - Standby/Resume</pc>: Records performance when the computer is placed on standby, and then resumed.</target>
