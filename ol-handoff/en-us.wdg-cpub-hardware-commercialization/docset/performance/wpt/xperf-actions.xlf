          <data id="id3">*</data>
          <data id="id4">*</data>
          <data id="id5">*</data>
          <data id="id6">*</data>
          <data id="id7">&amp;lt;</data>
          <data id="id8">\_</data>
          <data id="id9">&amp;gt;</data>
        </originalData>
        <segment state="initial">
          <source>Where <pc dataRefEnd="id2" dataRefStart="id1" id="p1">input.etl</pc> is the name of the trace file, <pc dataRefEnd="id4" dataRefStart="id3" id="p2">output.txt</pc> is the name of the report file, and <pc dataRefEnd="id6" dataRefStart="id5" id="p3"><ph dataRef="id7" id="ph1" />action<ph dataRef="id8" id="ph2" />name<ph dataRef="id9" id="ph3" /></pc> is the name of the action.</source>
