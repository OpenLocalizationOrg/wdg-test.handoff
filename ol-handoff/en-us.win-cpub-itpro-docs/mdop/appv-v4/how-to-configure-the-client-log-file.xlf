          <data id="id2">**</data>
        </originalData>
        <segment state="initial">
          <source>You must restart the <pc dataRefEnd="id2" dataRefStart="id1" id="p1">sftlist</pc> service after changing this value.</source>
          <target>You must restart the <pc dataRefEnd="id2" dataRefStart="id1" id="p1">sftlist</pc> service after changing this value.</target>
        </segment>
      </unit>
