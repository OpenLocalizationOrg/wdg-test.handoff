      <unit id="104">
        <segment state="initial">
          <source>XML files located on the server can be backed up and then restored in case of loss of data on the server.</source>
          <target>XML files located on the server can be backed up and then restored in case of loss of data on the server.</target>
        </segment>
      </unit>
      <unit id="105">
        <segment state="initial">
          <source>To back up a MED-V server</source>
          <target>To back up a MED-V server</target>
        </segment>
      </unit>
      <unit id="106">
        <originalData>
          <data id="id1">*</data>
          <data id="id2">*</data>
          <data id="id3">&amp;lt;</data>
          <data id="id4">&amp;gt;\\</data>
          <data id="id5">\\</data>
        </originalData>
        <segment state="initial">
          <source>Back up the following files, located in <pc dataRefEnd="id2" dataRefStart="id1" id="p1"><ph dataRef="id3" id="ph1" />InstallDir<ph dataRef="id4" id="ph2" />Servers<ph dataRef="id5" id="ph3" />ConfigurationServer</pc>:</source>
